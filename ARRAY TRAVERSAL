#include <stdio.h>

int main()
{
    int n;
    printf("Enter the Max no of elements in array\n");
    scanf("%d",&n);
    int arr[n];
    printf("Enter the elements of the array\n");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("Printing the elements of the array\n");
    for(int i=0;i<n;i++)
    {
        printf("at %d index = %d\n",i,arr[i]);
    }
}
